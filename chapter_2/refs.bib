number = {6},
pages = {646--652},
pmid = {12166861},
title = {{Regional lung perfusion as determined by electrical impedance tomography in comparison with electron beam CT imaging.}},
url = {http://dw2zn6fm9z.search.serialssolutions.com/?url{\_}ver=Z39.88-2004{\&}rft{\_}val{\_}fmt=info:ofi/fmt:kev:mtx:journal{\&}rfr{\_}id=info:sid/Ovid:med4{\&}rft.genre=article{\&}rft{\_}id=info:doi/{\&}rft{\_}id=info:pmid/12166861{\&}rft.issn=0278-0062{\&}rft.volume=21{\&}rft.issue=6{\&}rft.spage=646{\&}r},
