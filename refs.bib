	shortjournal = {Physiol. Meas.},
	author = {Nguyen, D. T. and Bhaskaran, A. and Chik, W. and Barry, M. A. and Pouliopoulos, J. and Kosobrodov, R. and Jin, C. and Oh, T. I. and Thiagalingam, A. and {McEwan}, A. L.},
