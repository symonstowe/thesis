	date = {1996-04},
	note = {Conference Name: {IEEE} Transactions on Medical Imaging},
	keywords = {Electrodes, Image reconstruction, Tomography, Surface impedance, Impedance measurement, Conductivity measurement, Electric variables measurement, Surface reconstruction, Frequency measurement, Noise measurement},
	file = {IEEE Xplore Full Text PDF:/home/symonstowe/Zotero/storage/WWNDM9CX/Adler and Guardo - 1996 - Electrical impedance tomography regularized imagi.pdf:application/pdf},
}

