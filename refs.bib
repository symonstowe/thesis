	keywords = {Image reconstruction, Tomography, Ventilation, Impedance, Conductivity, Lungs, mechanical ventilation, Biomedical imaging, electrical impedance tomography ({EIT}), Thorax, Medical treatment, Biomedical monitoring, blood gas, Patient monitoring, respiratory system},
