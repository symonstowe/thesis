	publisher = {{IEEE}},
	author = {Grychtol, Bartlomiej and Adler, Andy},
	urldate = {2021-06-28},
	date = {2013-07},
	langid = {english},
	keywords = {Electrodes, Tomography, Impedance, Sensitivity, Accuracy, Current measurement, Finite element analysis},
	file = {IEEE Xplore Abstract Record:/home/symonstowe/Zotero/storage/BZRV2UEE/6611026.html:text/html;Grychtol and Adler - 2013 - FEM electrode refinement for electrical impedance .pdf:/home/symonstowe/Zotero/storage/793HZWBU/Grychtol and Adler - 2013 - FEM electrode refinement for electrical impedance .pdf:application/pdf},
