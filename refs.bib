	pmcid = {PMC3466397},
	file = {PubMed Central Full Text PDF:/home/symonstowe/Zotero/storage/5HUWYS2I/Fedorov et al. - 2012 - 3D Slicer as an Image Computing Platform for the Q.pdf:application/pdf},
